{
    "_version": "1.40.0",
    "sap.app": {
        "id": "com.cnhi.btp.warrantyclaimsapproval",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.6.7",
            "toolsId": "30916546-2154-4693-98fb-45c78ba72af0"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZBTP_POC3_GTW_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "mockData": {
                "uri": "model/test.json",
                "type": "JSON"
            },
            "ZCLAIMAPPROVALSRV": {
                "uri": "/api/v2/srv/ZCLAIMAPPROVALSRV/",
                "type": "OData",
                "settings": {
                  "odataVersion": "4.0",
                  "localUri": "localService/metadata.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "com-cnhi-btp-warrantyclaimsapproval-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "claimsapproval",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": "sap-icon://validate"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "config": {
            "fullWidth": true
          },
        "dependencies": {
            "minUI5Version": "1.102.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.cnhi.btp.warrantyclaimsapproval.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultUpdateMethod": "PUT",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
              },
            "LocalModel": {
                "type": "sap.ui.model.json.JSONModel"    
            },
            "ClaimApprovalCAP": {
                "dataSource": "ZCLAIMAPPROVALSRV",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
                }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.cnhi.btp.warrantyclaimsapproval.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "main",
                    "pattern": "",
                    "target": [
                      "Main"
                    ]
                  },
                  {
                    "name": "detail",
                    "pattern": "detail/{claim}",
                    "target": [
                      "Detail"
                    ]
                  }
            ],
            "targets": {
                "Main": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Main",
                    "viewName": "Main"
                  },
                  "Detail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Detail",
                    "viewName": "Detail"
                  }
            }
        },
        "rootView": {
            "viewName": "com.cnhi.btp.warrantyclaimsapproval.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "com-cnhi-btp-warrantyclaimsapproval"
    }
}